//https://www.hackerrank.com/challenges/countingsort1/problem

#include <bits/stdc++.h>

using namespace std;



int main()
{
 map<int,int> arr;
 int n;
 cin >>n;
 for(int i=0;i<100;i++){
     arr.insert({i,0});
 }

 for(int i=0;i<n;i++){
     int num;
     cin >>num;
     arr[num]++;
 }
for(auto i=arr.begin();i!=arr.end();i++){
   cout <<i->second<<" ";
}

  return 0;

}
